            <TextBlock x:Name="Light1" HorizontalAlignment="Left" Height="34" Margin="87,60,0,0" Text="Light" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" TextDecorations="Underline" FontSize="24" RenderTransformOrigin="0.225,0.589"/>
            <TextBlock x:Name="Normal1" HorizontalAlignment="Left" Height="34" Margin="170,60,0,0" Text="Normal" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" Foreground="White" TextDecorations="Underline" FontSize="24" FontWeight="Bold"/>
            <TextBlock x:Name="Extra1" HorizontalAlignment="Left" Height="34" Margin="271,60,0,0" Text="Extra" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" TextDecorations="Underline" FontSize="24"/>

        </Grid>
        <Grid x:Name="Condiment3" HorizontalAlignment="Center" Height="94" Margin="0,402,0,0" VerticalAlignment="Top" Width="331">
            <TextBlock x:Name="Onions" HorizontalAlignment="Left" Height="29" Margin="119,0,0,0" Text="Onions" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" Foreground="White" FontSize="26"/>
            <TextBlock x:Name="None2" HorizontalAlignment="Left" Height="34" Margin="0,60,0,0" Text="None" TextWrapping="Wrap" VerticalAlignment="Top" Width="82" Foreground="White" TextDecorations="Underline" FontSize="24" FontWeight="Bold"/>
            <TextBlock x:Name="Light2" HorizontalAlignment="Left" Height="34" Margin="87,60,0,0" Text="Light" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" TextDecorations="Underline" FontSize="24" RenderTransformOrigin="0.225,0.589"/>
            <TextBlock x:Name="Normal2" HorizontalAlignment="Left" Height="34" Margin="170,60,0,0" Text="Normal" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" Foreground="White" TextDecorations="Underline" FontSize="24" FontWeight="Normal"/>
            <TextBlock x:Name="Extra2" HorizontalAlignment="Left" Height="34" Margin="271,60,0,0" Text="Extra" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Foreground="White" TextDecorations="Underline" FontSize="24"/>

        </Grid>
        <Rectangle Stroke="Black" Fill="#FFBA0000" RadiusX="35.333" RadiusY="35.333" Margin="12,25,12,36" Grid.Row="2">
            <Rectangle.Effect>
                <DropShadowEffect/>
            </Rectangle.Effect>
        </Rectangle>
        <TextBlock TextWrapping="Wrap" FontSize="26" Foreground="White" RenderTransformOrigin="0.955,0.455" Margin="147,41,87,53" Grid.Row="2" Text="Submit"/>

        <Frame x:Name="frame" Content="Frame" Margin="8,0,0,5" Grid.RowSpan="3" Width="367" Height="646"/>

    </Grid>
